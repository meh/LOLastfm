#! /sbin/runscript

depend() {
    need net
}

start() {
    ebegin "Starting LOLastfm"
    start-stop-daemon --start --quiet --exec "/usr/bin/LOLastfm" --make-pidfile --pidfile /tmp/LOLastfm.pid --background
    eend $?
}

stop() {
    ebegin "Stopping LOLastfm"
    start-stop-daemon --stop --quiet --pidfile /tmp/LOLastfm.pid
    eend $?
}
